<?xml version="1.0" encoding="UTF-8" ?>
<Commands>
  <DefaultLocale value="en"/>
  <!-- name is both the code class name (/src/Commands) and the file name for localisation (Case sensitive) -->
  <!-- commands are the console text to execute the command. Seperate each alias for the command with a comma -->
  <!-- defaultpermissionlevel can be overridden in the Saves/serveradmin.xml file, 0=admin level, 1000=anyone -->
  <Command
    name="ListPlayersExtensive"
    commands="bc-lpx,bc-lx"
    defaultpermissionlevel="0"
    />
  <Command
    name="ListPlayersGamestage"
    commands="bc-gs"
    defaultpermissionlevel="0"
    />
  <Command
    name="GiveQuestToPlayer"
    commands="bc-gqp"
    defaultpermissionlevel="0"
    />
  <Command
    name="RemoveQuestFromPlayer"
    commands="bc-rqp"
    defaultpermissionlevel="0"
    />
  <Command
    name="ListQuests"
    commands="bc-lq"
    defaultpermissionlevel="0"
    />

  <Command
    name="ListEntitiesData"
    commands="bc-le,bc-listentities"
    defaultpermissionlevel="0"
    />
  <Command 
    name="ListEntitiesDataJSON" 
    commands="bc-lejs" 
    defaultpermissionlevel="0"
    />
  <Command 
    name="ListPlayersAdvancedStats" 
    commands="bc-as" 
    defaultpermissionlevel="0"
    />
  <Command 
    name="ListPlayersData" 
    commands="bc-lp" 
    defaultpermissionlevel="0"
    />
  <Command
    name="Test"
    commands="bc-test"
    defaultpermissionlevel="0"
    />
</Commands>
